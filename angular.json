{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "kypo-visualization-dashboard": {
            "projectType": "library",
            "root": "projects/kypo-visualization-dashboard",
            "sourceRoot": "projects/kypo-visualization-dashboard/src",
            "prefix": "kypo",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/kypo-visualization-dashboard/tsconfig.lib.json",
                        "project": "projects/kypo-visualization-dashboard/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/kypo-visualization-dashboard/tsconfig.lib.prod.json"
                        },
                        "development": {}
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/kypo-visualization-dashboard/src/test.ts",
                        "tsConfig": "projects/kypo-visualization-dashboard/tsconfig.spec.json",
                        "karmaConfig": "projects/kypo-visualization-dashboard/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "projects/kypo-visualization-dashboard/**/*.ts",
                            "projects/kypo-visualization-dashboard/**/*.component.html"
                        ]
                    }
                }
            }
        },
        "kypo-visualization-dashboard-example": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:application": {
                    "strict": true
                }
            },
            "root": "projects/kypo-visualization-dashboard-example",
            "sourceRoot": "projects/kypo-visualization-dashboard-example/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/kypo-visualization-dashboard-example",
                        "index": "projects/kypo-visualization-dashboard-example/src/index.html",
                        "main": "projects/kypo-visualization-dashboard-example/src/main.ts",
                        "polyfills": "projects/kypo-visualization-dashboard-example/src/polyfills.ts",
                        "tsConfig": "projects/kypo-visualization-dashboard-example/tsconfig.app.json",
                        "assets": [
                            "projects/kypo-visualization-dashboard-example/src/favicon.ico",
                            "projects/kypo-visualization-dashboard-example/src/assets"
                        ],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                            "node_modules/normalize.css/normalize.css",
                            "projects/kypo-visualization-dashboard-example/src/styles.css"
                        ],
                        "scripts": [],
                        "vendorChunk": true,
                        "extractLicenses": false,
                        "buildOptimizer": false,
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/kypo-visualization-dashboard-example/src/environments/environment.ts",
                                    "with": "projects/kypo-visualization-dashboard-example/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        },
                        "local": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "projects/kypo-visualization-dashboard-example/src/environments/environment.ts",
                                    "with": "projects/kypo-visualization-dashboard-example/src/environments/environment.local.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "buildTarget": "kypo-visualization-dashboard-example:build"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "kypo-visualization-dashboard-example:build:production"
                        },
                        "local": {
                            "buildTarget": "kypo-visualization-dashboard-example:build:local"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "buildTarget": "kypo-visualization-dashboard-example:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/kypo-visualization-dashboard-example/src/test.ts",
                        "polyfills": "projects/kypo-visualization-dashboard-example/src/polyfills.ts",
                        "tsConfig": "projects/kypo-visualization-dashboard-example/tsconfig.spec.json",
                        "karmaConfig": "projects/kypo-visualization-dashboard-example/karma.conf.js",
                        "styles": [
                            "projects/kypo-visualization-dashboard-example/src/styles.css"
                        ],
                        "scripts": [],
                        "assets": [
                            "projects/kypo-visualization-dashboard-example/src/favicon.ico",
                            "projects/kypo-visualization-dashboard-example/src/assets"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "projects/kypo-visualization-dashboard-example/src/app/dashboard/**/*.ts",
                            "projects/kypo-visualization-dashboard-example/src/app/dashboard/**/*.component.html"
                        ]
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": false
    }
}
