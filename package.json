{
  "name": "@muni-kypo-crp/visualization-dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint && npm run prettier-check",
    "e2e": "ng e2e",
    "prettier-check": "prettier --check \"projects/**/*.ts\"",
    "prettier-fix": "prettier --write \"./projects/**/*.ts\"",
    "build-example-app": "ng build kypo-visualization-dashboard-example --configuration production",
    "build-library": "ng build --configuration production kypo-visualization-dashboard",
    "create-package": "cd dist/kypo-visualization-dashboard && npm pack",
    "build-and-pack": "npm install && npm run build-library && npm run create-package",
    "ci-update-version": "cd ./projects/kypo-visualization-dashboard/ && npm version $TAG_VERSION",
    "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-library && npm run create-package",
    "ci-publish-package": "cd dist/kypo-visualization-dashboard && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-check"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.3",
    "@angular/cdk": "^12.0.3",
    "@angular/common": "^12.0.3",
    "@angular/compiler": "^12.0.3",
    "@angular/core": "^12.0.3",
    "@angular/forms": "^12.0.3",
    "@angular/material": "^12.0.3",
    "@angular/platform-browser": "^12.0.3",
    "@angular/platform-browser-dynamic": "^12.0.3",
    "@angular/router": "^12.0.3",
    "@muni-kypo-crp/d3-service": "^12.0.2",
    "@muni-kypo-crp/hurdling-visualization": "~12.0.5",
    "@muni-kypo-crp/overview-visualization": "~12.1.0",
    "@muni-kypo-crp/command-visualizations": "~12.0.1",
    "@sentinel/auth": "~12.0.0",
    "@sentinel/common": "~12.2.1",
    "@sentinel/layout": "~12.0.0",
    "angular-oauth2-oidc": "^10.0.3",
    "angular-oauth2-oidc-jwks": "^9.0.0",
    "core-js": "^2.6.9",
    "d3": "^7.0.4",
    "d3-graphviz": "^2.6.1",
    "ngx-indexed-db": "^8.0.2",
    "normalize.css": "^8.0.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.5",
    "@angular/cli": "~12.2.5",
    "@angular/compiler-cli": "~12.2.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "^12.11.1",
    "@types/d3": "^7.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "~12.0.3",
    "protractor": "~7.0.0",
    "prettier": "2.3.0",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/schematics": "^12.0.0",
    "@angular-eslint/template-parser": "^12.0.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "eslint": "^7.13.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.3.5"
  }
}
