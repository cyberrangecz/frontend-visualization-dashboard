{
  "name": "kypo2-visualization-dashboard",
  "version": "0.9.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-example-app": "ng build example-app --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib-build": "ng build kypo2-visualization-dashboard-lib",
    "build-library": "ng build kypo2-visualization-dashboard-lib --configuration production && cp ./src/styles.css ./dist/kypo2-visualization-dashboard-lib",
    "create-package": "cd dist/kypo2-visualization-dashboard-lib && npm pack",
    "build-and-pack": "npm install && npm run build-library && npm run create-package",
    "ci-publish-package": "cd dist/kypo2-visualization-dashboard-lib && npm publish",
    "lib-copy": "cp -a ./dist/kypo2-visualization-dashboard-lib/. ../kypo2-visualization-dashboard-package/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.3",
    "@angular/cdk": "^12.0.3",
    "@angular/common": "^12.0.3",
    "@angular/compiler": "^12.0.3",
    "@angular/core": "^12.0.3",
    "@angular/forms": "^12.0.3",
    "@angular/material": "^12.0.3",
    "@angular/platform-browser": "^12.0.3",
    "@angular/platform-browser-dynamic": "^12.0.3",
    "@angular/router": "^12.0.3",
    "@muni-kypo-crp/d3-service": "~12.0.0",
    "@sentinel/auth": "~12.0.0",
    "@muni-kypo-crp/overview-visualization": "12.0.6",
    "@muni-kypo-crp/hurdling-visualization": "12.0.3",
    "angular-oauth2-oidc": "^10.0.3",
    "angular-oauth2-oidc-jwks": "^9.0.0",
    "core-js": "^2.6.9",
    "d3": "^5.16.0",
    "normalize.css": "^8.0.1",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.3",
    "@angular/cli": "~12.0.3",
    "@angular/compiler-cli": "^12.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "^12.11.1",
    "@types/d3": "^5.7.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.3",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "typescript": "~4.2.4"
  }
}
