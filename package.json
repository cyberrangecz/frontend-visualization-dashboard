{
  "name": "@muni-kypo-crp/visualization-dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint && npm run prettier-check",
    "e2e": "ng e2e",
    "prettier-check": "prettier --check \"projects/**/*.ts\"",
    "prettier-fix": "prettier --write \"./projects/**/*.ts\"",
    "build-example-app": "ng build kypo-visualization-dashboard-example --configuration production",
    "build-library": "ng build --configuration production kypo-visualization-dashboard",
    "create-package": "cd dist/kypo-visualization-dashboard && npm pack",
    "build-and-pack": "npm install && npm run build-library && npm run create-package",
    "ci-update-version": "cd ./projects/kypo-visualization-dashboard/ && npm version $TAG_VERSION",
    "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-library && npm run create-package",
    "ci-publish-package": "cd dist/kypo-visualization-dashboard && npm publish",
    "prepare": "husky install",
    "json-server": "json-server -w utils/json-server/db.json -r utils/json-server/routes.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@muni-kypo-crp/command-visualizations": "~14.1.0",
    "@muni-kypo-crp/d3-service": "^15.0.1",
    "@muni-kypo-crp/hurdling-visualization": "~14.2.0",
    "@muni-kypo-crp/overview-visualization": "~15.0.0",
    "@sentinel/auth": "~15.3.0",
    "@sentinel/common": "~15.3.0",
    "@sentinel/layout": "~15.2.0",
    "angular-oauth2-oidc": "^15.0.1",
    "angular-oauth2-oidc-jwks": "^15.0.1",
    "core-js": "^3.23.3",
    "d3": "^7.6.1",
    "d3-graphviz": "^4.1.1",
    "rxjs": "~7.5.5",
    "ngx-indexed-db": "^11.0.2",
    "normalize.css": "^8.0.1",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@types/jasminewd2": "^2.0.10",
    "@types/jasmine": "~4.0.3",
    "@types/node": "^18.0.3",
    "@types/d3": "^7.4.0",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "protractor": "~7.0.0",
    "ng-packagr": "^15.2.2",
    "prettier": "^2.7.1",
    "@angular-eslint/builder": "~15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "husky": "^8.0.1",
    "eslint": "^8.19.0",
    "ts-node": "~10.8.2",
    "typescript": "~4.9.5"
  }
}
